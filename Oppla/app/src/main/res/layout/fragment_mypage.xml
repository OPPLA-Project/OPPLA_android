<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".view.main.mypage.mypagehome.MypageFragment">

        <include
            android:id="@+id/mypage_toolbar"
            layout="@layout/toolbar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/mypage_textview_setting"
            android:layout_width="wrap_content"
            android:layout_height="55dp"
            android:layout_marginHorizontal="16dp"
            android:layout_marginTop="16dp"
            android:gravity="center"
            android:paddingHorizontal="8dp"
            android:text="설정"
            android:textColor="#313336"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/mypage_toolbar" />

        <View
            android:id="@+id/mypage_view_line1"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginHorizontal="16dp"
            android:background="#EEF1F5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/mypage_textview_setting" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/mypage_textview_profile"
            android:layout_width="wrap_content"
            android:layout_height="55dp"
            android:layout_marginHorizontal="16dp"
            android:gravity="center"
            android:paddingHorizontal="8dp"
            android:text="프로필"
            android:textColor="#313336"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/mypage_view_line1" />

        <View
            android:id="@+id/mypage_view_line2"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginHorizontal="16dp"
            android:background="#EEF1F5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/mypage_textview_profile" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/mypage_textview_history"
            android:layout_width="wrap_content"
            android:layout_height="55dp"
            android:layout_marginHorizontal="16dp"
            android:gravity="center"
            android:paddingHorizontal="8dp"
            android:text="거래내역"
            android:textColor="#313336"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/mypage_view_line2" />

        <View
            android:id="@+id/mypage_view_line3"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginHorizontal="16dp"
            android:background="#EEF1F5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/mypage_textview_history" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/mypage_textview_announcement"
            android:layout_width="wrap_content"
            android:layout_height="55dp"
            android:layout_marginHorizontal="16dp"
            android:gravity="center"
            android:paddingHorizontal="8dp"
            android:text="공지사항"
            android:textColor="#313336"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/mypage_view_line3" />

        <View
            android:id="@+id/mypage_view_line4"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginHorizontal="16dp"
            android:background="#EEF1F5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/mypage_textview_announcement" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/mypage_textview_inquiry"
            android:layout_width="wrap_content"
            android:layout_height="55dp"
            android:layout_marginHorizontal="16dp"
            android:gravity="center"
            android:paddingHorizontal="8dp"
            android:text="문의하기"
            android:textColor="#313336"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/mypage_view_line4" />

        <View
            android:id="@+id/mypage_view_line5"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginHorizontal="16dp"
            android:background="#EEF1F5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/mypage_textview_inquiry" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>