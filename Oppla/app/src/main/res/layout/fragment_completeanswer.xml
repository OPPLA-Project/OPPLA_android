<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".view.main.question.completeanswer.CompleteanswerFragment">

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/completeanswer_textview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="전송완료"
            android:textSize="30sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/completeanswer_textview_complete"
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/completeanswer_textview"
            app:layout_constraintEnd_toEndOf="parent"
            android:text="완료"/>

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
